                    var mediaUrl = attachments[i].get('attachment').url() ;
                    <!--get the file name from a field DB-->
                    <!--var fileNameDDD = attachments[i].get('fileName');-->
                    var found= mediaUrl.lastIndexOf('-') ;
                    found= found==-1?0:found;
                    var fileName= decodeURI(mediaUrl.substring(found+1));
                    var foundVideo =mediaUrl.indexOf('.mp4')>0;
                    var foundImage =mediaUrl.indexOf('.jpg')>0||mediaUrl.indexOf('.png')>0||mediaUrl.indexOf('.bmp')>0||mediaUrl.indexOf('.gif')>0;
                    %>

                    <!--<img class='clickable_image'  src="<%=attachments[i].get('attachment').url()%>" />-->

                    <div id="divAttachment<%=index%>" class="divAttachment" style="width:500px;height:220px;">
