                        <!--<input type="file" style="display:inline" name="attachment<%=index%>" id="attachment<%=index%>" class="attachment" value="<%=attachments[i].get('attachment').url() %>" />-->
                          <%if(foundVideo){%>
                            <div style="height:150px;float:left;display:inline;"> <video controls class='media-video'  src="<%=mediaUrl%>" style="width:150px;height:150px"></video></div>
